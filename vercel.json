{
  "version": 2,
  "builds": [
      { "src": "server.js", "use": "@vercel/node" },
      { "src": "public/**", "use": "@vercel/static" }
  ],
  "rewrites": [
      { "source": "/api/(.*)", "destination": "/server.js" },
      { "source": "/(.*)", "destination": "/public/$1" }
  ],
  "engines": { "node": "18.x" },
  "env": { "NODE_ENV": "production" },
  "alias": "english-learning-website-olive.vercel.app"
}